{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/app/$1"
    }
  ],
  "redirects": [
    {
      "source": "/old-url",
      "destination": "/new-url",
      "statusCode": 301
    }
  ]
}
