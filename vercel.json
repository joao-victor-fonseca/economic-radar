{
  "version": 2,
  "builds": [
    {
      "src": "app/(root)/(home)/**",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/dashboard/:city",
      "destination": "/app/(root)/(home)/dashboard/:city/page.tsx"
    }
  ]
}
