{
  "version": 2,
  "builds": [
    {
      "src": "app/(root)/(home)/**",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/app/(root)/(home)/:path*"
    }
  ]
}
